enum WaveType {
    WUS,
    MID,
    CHAD  
}

struct SurferData {
    WaveType waveType;
    bool finishedSurfing;
    uint64 cooldown;
    uint256 entropy;
}

contract cn24 is Ownable, ERC721AQueryable, ReentrancyGuard {

    uint public constant MAX_SUPPLY = 100; //update to 3000
    iWave constant wave = iWave(0x1234); //update to mainnet waveContract

    string public baseTokenURI = "";
    string public unrevealedURI = "ipfs://"; //update to unrevealed IPFS
    
    constructor() ERC721A("cn24nia", "cn24") {}

    /*
     * SURFING
     */

    uint public constant cantoRecruitFee = 1 ether; //update to 69 canto
    uint public constant maxForCantoHire = 60; //update to 2000
    uint public constant waveRecruitFee = 100 ether; //update to 1000
    uint public constant maxForWaveHire = 40; //update to 1000
    uint64 public constant surferCooldown = 60 seconds; //update to 24 hours

    uint public cantoTotalHired;
    uint public waveTotalHired;

    uint public baseWusFee = 1 ether;
    uint public baseMidFee = 16 ether;
    uint public baseChadFee = 80 ether;

    bool public readyForHire;

    function hireSurfer(uint totalSurfers, bool payWithWave) public payable nonReentrant {
        if (totalSurfers > 10) revert TooManySurfers();
        if (!readyForHire) revert HiringNotStarted();

        uint totalMinted = _totalMinted();
        if (totalMinted == MAX_SUPPLY) revert MaxSupplyReached();
        if (totalMinted + totalSurfers > MAX_SUPPLY) {
            totalSurfers = MAX_SUPPLY - totalMinted;
        }

        if (payWithWave == false) {
            cantoTotalHired = cantoTotalHired + totalSurfers;
            if (cantoTotalHired > maxForCantoHire) revert MaxCantoSupplyReached();

            uint expectedValue = totalSurfers * cantoRecruitFee;
            if (msg.value < expectedValue) revert WrongPrice();
            if (msg.value > expectedValue) {
                if (!_transferETH(msg.sender, msg.value - expectedValue)) revert FailedToRefund();
            }

            wave.mint(msg.sender, 1 ether * totalSurfers);
        }
        else {
            waveTotalHired = waveTotalHired + totalSurfers;
            if (waveTotalHired > maxForWaveHire) revert MaxWaveSupplyReached();
            wave.burn(msg.sender,waveRecruitFee*totalSurfers);
            updateBeachFees();
        }

        _mint(msg.sender, totalSurfers);
    }

    uint public wusWaveFee = 1 ether;
    uint public midWaveFee = 16 ether;
    uint public chadWaveFee = 80 ether; //slippage needs to be added to fees to balance EV

    function updateBeachFees() internal {
        wusWaveFee = baseWusFee*(maxForWaveHire-waveTotalHired)/maxForWaveHire;
        midWaveFee = baseMidFee*(maxForWaveHire-waveTotalHired)/maxForWaveHire;
        chadWaveFee = baseChadFee*(maxForWaveHire-waveTotalHired)/maxForWaveHire;
    }

    uint public allTimeSurfs;
    mapping (uint => SurferData) surferData;

    function surfBrah(uint[] memory surfers, WaveType waveType) external nonReentrant {
        if(!beachIsOpen) revert BeachNotOpen();

        allTimeSurfs = allTimeSurfs + surfers.length;

        if (waveType == WaveType.WUS) {
            wave.burn(msg.sender,wusWaveFee*surfers.length);
        }
        else if (waveType == WaveType.MID) {
            wave.burn(msg.sender,midWaveFee*surfers.length);
        }
        else if (waveType == WaveType.CHAD) {
            wave.burn(msg.sender,chadWaveFee*surfers.length);
        }

        for(uint i = 0; i < surfers.length; i++) {
            if (ownerOf(surfers[i]) != msg.sender) revert NotOwner();

            SurferData memory surfer = surferData[surfers[i]];

            if (surfer.cooldown > block.timestamp) revert SurferTired();

            surfer.cooldown = uint64(block.timestamp) + surferCooldown;
            surfer.entropy = uint(keccak256(abi.encodePacked(block.timestamp,block.difficulty,surfers[i])));
            surfer.waveType = waveType;
            surfer.finishedSurfing = false;

            surferData[surfers[i]] = surfer;
        }
    }

    function finishSurfing(uint[] memory surfers) external nonReentrant {
        if(beachIsOpen) revert BeachMustBeClosed();

        uint howManyWavesBro = 0;

        for(uint i = 0; i < surfers.length; i++) {
            if (ownerOf(surfers[i]) != msg.sender) revert NotOwner();

            SurferData memory surfer = surferData[surfers[i]];

            if (surfer.finishedSurfing) revert AlreadyFinished();
            if (surfer.cooldown <= block.timestamp) revert ForgotYourSurfer();
            
            howManyWavesBro = howManyWavesBro + catchWaves(surfer.waveType, surfer.entropy);

            surferData[surfers[i]].finishedSurfing = true;
        }

        wave.mint(msg.sender, howManyWavesBro*(maxForWaveHire-waveTotalHired)/maxForWaveHire);
    }

    function catchWaves(WaveType waveType, uint entropy) internal view returns (uint wavesCaught) {
        uint8 salt = uint8(uint(keccak256(abi.encodePacked(entropy, weatherConditions)))%100)+1;

        uint earnedIndex = 0;
        uint8[10] memory rarity;
        uint16[10] memory wavesEarned;

        if(waveType == WaveType.WUS) {
            rarity = [0,1,3,13,33,67,87,97,99,100];
            wavesEarned = [uint16(0),5,8,12,14,16,18,20,24,29];
        }
        else if(waveType == WaveType.MID) {
            rarity = [0,3,11,26,49,73,88,96,99,100];
            wavesEarned = [uint16(0),1,16,25,34,39,42,69,88,111];
        }
        else if (waveType == WaveType.CHAD) {
            rarity = [0,7,19,35,60,76,88,95,99,100];
            wavesEarned = [uint16(0),1,42,69,88,111,169,269,333,420];
        }

        for (uint i = 0; i < 10; i++) {
            if (salt >= rarity[i]) {
                earnedIndex = i;
            } else {
                break;
            }
        }

        wavesCaught = wavesEarned[earnedIndex] * 1 ether;
    }

    /*
     * CHAD SALE
     */

    bool public chadSaleActive;
    uint public chadSaleTimer;
    uint public waveForChad;

    address public biggestChad;
    mapping(address => string) public chadNames;

    uint constant startingWaveForChad = 42 ether; //set to 420
    uint constant chadCompLength = 180 seconds; //set to 24 hours

    function startChadSale() public onlyOwner {
        chadSaleActive = true;
        chadSaleTimer = block.timestamp + chadCompLength;
        waveForChad = startingWaveForChad; 
    }

    function endChadSale() public onlyOwner {
        chadSaleActive = false;
    }

    function chadSale(uint maxBid) public nonReentrant {
        if (!chadSaleActive) revert ChadNotActive();
        if (chadSaleTimer < block.timestamp) revert ChadCompetitionOver();
        if (maxBid <= waveForChad) revert MaxBidTooLow();

        wave.burn(msg.sender, waveForChad);
        address previousChad = biggestChad;

        if(waveForChad > startingWaveForChad) {
            wave.mint(previousChad,waveForChad*95/100);
        }

        waveForChad = waveForChad*110/100;
        biggestChad = msg.sender;
    }    


    function chadsRealName(string memory chadsName) public {
        chadNames[msg.sender] = chadsName;
    }

    /*
     * ADMIN
     */

    function withdraw() external onlyOwner {
        bool success = _transferETH(msg.sender, address(this).balance);
        if (!success) {
            revert WithdrawFailed();
        }
    }

    function updateBaseFees(uint newWusFee, uint newMidFee, uint newChadFee) external onlyOwner {
        baseWusFee = newWusFee;
        baseMidFee = newMidFee;
        baseChadFee = newChadFee;
    }

    function allowHiring() external onlyOwner {
        readyForHire = true;
    }

    function setUnrevealedURI(string memory uri) external onlyOwner {
        unrevealedURI = uri;
    }

    function setBaseURI(string calldata uri) external onlyOwner {
        baseTokenURI = uri;
    }

    function updateLifeGuardFee(uint newFees) external onlyOwner {
        feesForLifeGuards = newFees;
    }

    /*
     * TIMEKEEPING
     */

    uint public hourOffset = 8;

    function updateOffset(uint newOffset) public onlyOwner {
        hourOffset = newOffset; //update for daylight savings
    }

    function subHours(uint timestamp, uint _hours) public pure returns (uint newTimestamp) {
        newTimestamp = BokkyPooBahsDateTimeLibrary.subHours(timestamp, _hours);
    }

    function getDayAndTime(uint timestamp) public view returns (uint hour, uint minute, bool weekend) {
        uint offsetTimestamp = subHours(timestamp, hourOffset); //set to PST

        hour = BokkyPooBahsDateTimeLibrary.getHour(offsetTimestamp);
        minute = BokkyPooBahsDateTimeLibrary.getMinute(timestamp);
        weekend = BokkyPooBahsDateTimeLibrary.isWeekEnd(timestamp);
    }

    function checkCantoHours(uint timestamp) public view returns (bool beachReady) {
        (uint hour, uint minute, bool weekend) = getDayAndTime(timestamp);

        if(weekend == true) {
            beachReady = false;
        }
        else if ((hour >= 11) && (hour < 4)) {
            beachReady = true;
        }
        else if ((hour == 4) && (minute <= 20)) {
            beachReady = true;
        }
        else {
            beachReady = false;
        }
    }

    /*
     * LIFEGUARDS
     */

    bool public beachIsOpen;
    uint public feesForLifeGuards = 16.9 ether;
    uint public weatherConditions;

    function openBeach() external {
        if(beachIsOpen == true) revert BeachAlreadyOpen();
        if(checkCantoHours(block.timestamp) == false) revert BlockchainClosed();

        beachIsOpen = true;
        wave.mint(msg.sender, feesForLifeGuards);
    }

    function closeBeach() external {
        if(beachIsOpen == false) revert BeachAlreadyClosed();
        if(checkCantoHours(block.timestamp) == true) revert BlockchainStillOpen();

        beachIsOpen = false;
        wave.mint(msg.sender, feesForLifeGuards);
        weatherConditions = uint(keccak256(abi.encodePacked(block.timestamp,block.difficulty)));
    }

    /*
     * VIEWS
     */

    function surfStats() external view returns (uint totalSurfs, uint totalSurfers) {
        totalSurfs = allTimeSurfs;
        totalSurfers = totalSupply();
    }

    function ownerSurfersReady(address owner) external view returns (uint[] memory readySurfers) {
        uint count = 0;
        uint surfersOwnedByAddress = balanceOf(owner);

        for (uint surfer = 1; surfer <= surfersOwnedByAddress; surfer++) {
            if (ownerOf(surfer) == owner && surferData[surfer].cooldown <= block.timestamp) {
                count++;
            }
        }

        readySurfers = new uint[](count);
        uint index = 0;

        for (uint surfer = 1; surfer <= surfersOwnedByAddress; surfer++) {
            if (ownerOf(surfer) == owner && surferData[surfer].cooldown <= block.timestamp) {
                readySurfers[index] = surfer;
                index++;
            }
        }
    }

    function waveSlippage() external view returns (uint slippage) {
        slippage = 1 ether * (maxForWaveHire-waveTotalHired) / maxForWaveHire;
    }

    function biggestChadName() public view returns (string memory) {
        return chadNames[biggestChad];
    }

    /*
     * INTERNALS
     */

    function _transferETH(address to, uint value) internal returns (bool) {
        (bool success,) = to.call{value: value, gas: 30000}(new bytes(0));
        return success;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseTokenURI;
    }
